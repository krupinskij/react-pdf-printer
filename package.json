{
  "name": "react-pdf-printer",
  "version": "1.0.0",
  "description": "Create pdf document using React",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
