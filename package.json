  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.0",
    "babel-loader": "^8.2.4",
    "husky": "^7.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
